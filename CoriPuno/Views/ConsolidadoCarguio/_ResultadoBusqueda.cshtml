@model List<CoriPuno.Entidad.ProgramacionDiariaEntidad>
    @{
        ViewBag.Title = "_ResultadoBusqueda";
    }

@if (Model != null && Model.Count > 0)
{

    <table id="keywords" cellspacing="0" cellpadding="0">
        <thead>
            <tr>
                <th><span>Labor Destino</span></th>
                <th align="center"><span>Ley</span></th>
                <th align="center"><span>Programado</span></th>
                <th align="center"><span>Ejecutado</span></th>
                <th align="center"><span>Por Variacion</span></th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td align="center">@item.LaborDestino</td>
                    <td align="center">@item.Ley</td>
                    <td align="center">@item.Programado</td>
                    <td align="center">@item.Ejecutado</td>
                    <td align="center">@item.PorVariacion</td>
                </tr>
            }
        </tbody>
    </table>
}

