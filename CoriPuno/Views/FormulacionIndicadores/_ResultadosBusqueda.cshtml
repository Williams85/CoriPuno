@model List<CoriPuno.Entidad.IndicadorEntidad>
    @{
        ViewBag.Title = "_ResultadosBusqueda";
    }

    @if (Model != null && Model.Count > 0)
    {

        <table id="keywords" cellspacing="0" cellpadding="0">
            <thead>
                <tr>
                    <th><span>Nombre Indicador</span></th>
                    <th><span>Variable Indicador</span></th>
                    <th><span>Formula</span></th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <th>@item.Nom_Indicador</th>
                        <th>@item.Var_Indicador</th>
                        <td>@item.Formula</td>
                        <td>
                            @using (Html.BeginForm("Edicion", "FormulacionIndicadores", FormMethod.Post, new { @role = "form" }))
                            {
                                <input type="hidden" id="Codigo" name="Codigo" value="@item.Id_Indicador" />
                                <button type="submit" class="btn btn-primary btn-sm"><span class="separarboton glyphicon glyphicon-pencil"></span>Editar</button>
                            }
                        </td>

                    </tr>
                }
            </tbody>
        </table>
    }
    else
    {
        <span>No hay resultados</span>
    }

